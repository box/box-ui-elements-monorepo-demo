{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "start": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**",".next/**"]
    },
    "dev": {
      "cache": false
    },
    "install": {
      "cache": false
    },
    "preview": {
      "cache": false
    }
  }
}
